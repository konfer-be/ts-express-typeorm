{
  "name": "ts-express-typeorm-boilerplate",
  "engines": {
    "node": ">=10.9"
  },
  "version": "1.0.1",
  "author": "Steve Lebleu",
  "license": "ISC",
  "description": "This repository contains REST API boilerplate Typescript / Express.js / TypeORM based.",
  "keywords": [
    "node",
    "express.js",
    "typescript",
    "typeorm",
    "mysql",
    "boilerplate",
    "api",
    "rest",
    "restful",
    "es2017"
  ],
  "homepage": "https://github.com/konfer-be/ts-express-typeorm-boilerplate#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/konfer-be/ts-express-typeorm-boilerplate.git"
  },
  "bugs": {
    "url": "https://github.com/konfer-be/ts-express-typeorm-boilerplate/issues"
  },
  "main": "./dist/api/app.bootstrap.js",
  "lint": "tslint --project tslint.json",
  "scripts": {
    "kickstart": "npm i typescript -g && npm i typeorm -g && npm i kfr-kem -g && npm i && npm-run-all -s kick:*",
    "kick:directories": "mkdir -p ./dist/logs ./dist/uploads/archives ./dist/uploads/documents ./dist/uploads/images/master-copy ./dist/uploads/images/rescale/extra-small ./dist/uploads/images/rescale/medium ./dist/uploads/images/rescale/extra-large",
    "kick:env": "cp -r ./src/env ./dist/",
    "test": "nyc --reporter=html --report-dir=./docs/nyc-coverage ./node_modules/.bin/mocha --exit --reporter spec --timeout 10000",
    "coverage": "plato -r -t 'API code coverage report' -d ./docs/plato-coverage ./dist",
    "todo-ci": "leasot -x --reporter markdown './src/**/*.ts' > TODO.md",
    "apidoc-ci": "apidoc -i src/ -o docs/apidoc/",
    "typedoc-ci": "typedoc --mode modules --name 'API documentation' --defaultCategory 'Root' --categorizeByGroup  --out ./docs/typedoc ./src"
  },
  "_moduleAliases": {
    "@bases": "dist/api/types/classes",
    "@config": "dist/config",
    "@controllers": "dist/api/controllers",
    "@enums": "dist/api/types/enums",
    "@errors": "dist/api/types/errors",
    "@interfaces": "dist/api/types/interfaces",
    "@middlewares": "dist/api/middlewares",
    "@models": "dist/api/models",
    "@repositories": "dist/api/repositories",
    "@routes": "dist/api/routes/v1",
    "@serializers": "dist/api/serializers",
    "@servers": "dist/servers",
    "@services": "dist/api/services",
    "@utils": "dist/api/utils",
    "@validations": "dist/api/validations",
    "@whitelists": "dist/api/serializers/whitelists"
  },
  "dependencies": {
    "@types/body-parser": "^1.17.0",
    "@types/express": "^4.16.0",
    "@types/node": "^12.7.2",
    "awilix": "^4.2.2",
    "axios": "^0.21.1",
    "bcrypt": "^5.0.0",
    "body-parser": "^1.18.3",
    "boom": "^7.3.0",
    "compression": "^1.7.3",
    "cors": "^2.8.5",
    "dotenv": "^8.1.0",
    "errorhandler": "^1.5.0",
    "es6-promisify": "^6.0.1",
    "express": "^4.16.4",
    "express-hbs": "^2.3.3",
    "express-rate-limit": "^5.0.0",
    "express-validation": "^1.0.2",
    "express-validator": "^6.1.1",
    "filenamify": "^4.1.0",
    "helmet": "^3.23.3",
    "hpp": "^0.2.2",
    "http-status": "^1.3.1",
    "jimp": "^0.14.0",
    "joi": "^14.3.1",
    "jsonapi-serializer": "^3.6.4",
    "jwt-simple": "^0.5.5",
    "lodash": "^4.17.19",
    "module-alias": "^2.2.1",
    "moment-timezone": "^0.5.23",
    "morgan": "^1.9.1",
    "multer": "^1.4.1",
    "mysql": "^2.17.1",
    "passport": "^0.4.0",
    "passport-http-bearer": "^1.0.1",
    "passport-jwt": "^4.0.0",
    "pluralize": "^8.0.0",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.2.18",
    "uuid": "^3.3.2",
    "winston": "^3.1.0"
  },
  "devDependencies": {
    "apidoc": "^0.17.7",
    "chai": "^4.2.0",
    "chance": "^1.0.18",
    "leasot": "^8.0.0",
    "mocha": "^6.2.0",
    "nock": "^10.0.6",
    "node-notifier": "^8.0.1",
    "npm-run-all": "^4.1.5",
    "nyc": "^14.1.1",
    "supertest": "^4.0.2",
    "typedoc": "^0.15.0",
    "typescript": "^3.5.3"
  }
}
